{
  "name": "qingguowebsite",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^2.13.4",
    "axios": "^0.17.1",
    "echarts-for-react": "^2.0.0",
    "echarts-wordcloud": "^1.1.2",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.4",
    "node-sass-chokidar": "^0.0.3",
    "rc-queue-anim": "^1.3.0",
    "rc-scroll-anim": "^1.0.7",
    "rc-tween-one": "^1.5.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-photo-gallery": "^6.0.17",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-scroll": "^0.4.3",
    "react-scripts": "1.0.14",
    "redux": "^3.7.2",
    "vivus": "^0.4.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-app-rewired start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "npm run clean && react-app-rewired build",
    "build": "npm-run-all build-css build-js",
    "deploy": "npm run build && cp -a build/. .",
    "clean": "rimraf build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.6.0",
    "eslint-config-airbnb": "^15.1.0",
    "react-app-rewired": "^1.3.1",
    "rimraf": "^2.6.2"
  }
}
